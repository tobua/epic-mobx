{
  "name": "epic-mobx",
  "description": "Makes it easy to work with lists of nested stores in MobX.",
  "version": "0.0.0-development",
  "repository": "tobua/epic-mobx",
  "homepage": "https://tobua.github.io/epic-mobx",
  "license": "MIT",
  "author": "Matthias Giger",
  "scripts": {
    "build": "padua build",
    "start": "padua watch",
    "test": "padua test"
  },
  "peerDependencies": {
    "mobx": "^6.5.0"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "mobx",
    "store",
    "nested",
    "react"
  ],
  "devDependencies": {
    "mobx": "^6.5.0",
    "padua": "^0.5.4"
  },
  "prettier": "padua/configuration/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/padua/configuration/eslint.cjs"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "./tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "engines": {
    "node": ">= 14"
  }
}
